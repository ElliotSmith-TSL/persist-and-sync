{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"persist-and-sync#build": {
			"cache": false
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
